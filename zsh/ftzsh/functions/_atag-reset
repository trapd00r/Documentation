#compdef atag-reset
###
### @@webhtml_contact@@
### Last-Modified: @@webhtml_last_modified@@
###
### URI: <@@webhtml_uri@@>
###
local arguments
local -a supported_extensions

supported_extensions=( 'mp3' 'ogg' 'flac' )

function _ar_files() {
    _path_files -g "(#i)*.(${(j:|:)supported_extensions})" -g "*(-/)"
}

arguments=( '*::audio files:_ar_files' )
_arguments -s $arguments
