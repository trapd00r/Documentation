Thanks I've just added an echo

function cpn()
{
  setopt localoptions
  setopt globassign
  local nf nd
  [[ -z ${1} || -z ${2} ]] && printf 'usage: cpn <fromdir> <todir>\n' && 
return 1
  nf=${1}/*(.om[1]N)
  nd=${2}/*(/om[1]N)
  result="cp $nf $nd"
  [[ -n ${nd} && -n ${nf} ]] && command cp ${nf} ${nd} ; echo $result
}


