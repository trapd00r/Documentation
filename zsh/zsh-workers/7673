> _perl_pods {
>     local nm="$compstate[nmatches]"
> 
>    _perl_modules
>    _perl_basepods
>    _path_files -J "Perl modules and .pods" -/ -g '*.(pod|pm)'
> 
>     [[ nm -ne compstate[nmatches] ]]
> }

