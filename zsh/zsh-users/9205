>  i=3D"mc-4.6.1-2005-07-25-14.diff.bz2"
>  echo $i
>  --> mc-4.6.1-2005-07-25-14.diff.bz2
>  echo $i(:r)
>  --> mc-4.6.1-2005-07-25-14.diff
