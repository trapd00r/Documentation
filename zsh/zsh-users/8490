What killed my setopt (was in my .zshenv)

# complete man pages
setopt SH_WORD_SPLIT function man_var () {
   man_pages=( $^manpath/man*/*(N:t:r:r) )
   compctl -k man_pages man
   reply=( $man_pages )
}
compctl -K man_var man; man_pages=()

