     0) return 1 ;;
-    1) _files "$opts[@]" -g "$glob_patterns[0]" ;;
+    1) _files "$opts[@]" -g "$glob_patterns[1]" ;;
     *) _files "$opts[@]" -g "{${(j/,/)glob_patterns}}" ;;
   esac
 }
