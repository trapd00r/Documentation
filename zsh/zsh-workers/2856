  # now <Esc>-h works for shell builtins.
  
  while (<>) {
!     last if /^SHELL BUILTIN COMMANDS/;
      /zshbuiltins/ && $zb++;
      last if ($zb && /^\s*DESCRIPTIONS/);
  }
