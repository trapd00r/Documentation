>  fi
>  
> -if [[ $_acroread_version == 7.* ]]; then
> +if [[ $_acroread_version == [78].* ]]; then
> +  local -a extra_args
> +  if [[ $_acroread_version == 8.* ]]; then
> +    extra_args=(-man '-installCertificate:server ip::server port')
> +  else
> +    extra_args=()
> +  fi
>    _arguments -C \
> +    "${extra_args[@]}" \
>      '--display=:X display:_x_display' \
