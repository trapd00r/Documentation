Content-Type: text/plain;
  charset="UTF-8";
  name="run-help-sudo"
Content-Transfer-Encoding: 7bit
Content-Disposition: attachment;
	filename="run-help-sudo"


if [ $# -eq 0 ]; then
    man sudo
else
    man $1
fi


