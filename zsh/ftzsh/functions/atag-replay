### vim:ft=zsh:foldmethod=marker

if (( # < 1 )); then
    printf 'usage: atag-replay <file> [OPTION(s)...]\n'
    return 2
fi

local file
file=$1
shift

atag ${file} "$@" \
    al=${_al}     \
    ar=${_ar}     \
    tt=${_tt}     \
    tn=${_tnp}    \
    y=${_yr}      \
    g=${_gr}      \
    comp=${_comp}

return $?
