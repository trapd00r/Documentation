#compdef gnupod_check gnupod_check.pl

# completions for gnupod_check.pl version 0.98.3
# The author names this file gnupod_check.pl; however Debian
# GNU/Linux strips off the .pl extension

local arguments

arguments=(
    '(-h --help)'{-h,--help}'[display help and exit]'
    --version'[output version information and exit]'
    '(-m --mount)'{-m,--mount=}'[iPod mountpoint (default is $IPOD_MOUNTPOINT)]:mount point:_files'
    --fixit'[try to fixup some errors]'
    )

_arguments : $arguments

