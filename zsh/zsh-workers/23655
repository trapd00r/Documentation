+_git-instaweb () {
+  _arguments \
+    '(-l --local)'{-l,--local}'[bind the web server to 127.0.0.1]' \
+    '(-d --httpd)'{-d,--httpd=3D}'[HTTP-daemon command-line that will be e=
xecuted]:command line' \
+    '(-m --module-path)'{-m,--module-path=3D}'[module path for the Apache =
HTTP-daemon]:module path:_directories' \
+    '(-p --port)'{-p,--port=3D}'[port to bind web server to]: :_guard "[[\=
:digit\:]]" port' \
+    '(-b --browser)'{-b,--browser=3D}'[web-browser command-line that will =
be executed]:command line' \
+    '--start[start the HTTP-daemon and exit]' \
+    '--stop[start the HTTP-daemon and exit]' \
+    '--restart[restart the HTTP-daemon and exit]' && ret=3D0
+}
