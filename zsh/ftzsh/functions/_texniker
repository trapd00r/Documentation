#compdef tn

local line
typeset -ga _arename_types
typeset -ga _arename_extensions

function _texniker_modes() {
    local expl
    local -a modes
    modes=( ${${(s: :)"$(tn -L)"}} )
    _describe -t modes 'document modes' modes
}

function _texniker() {
    _arguments -s \
        '-l[list modes]:' \
        '-L[list modes in a machine readable way]:' \
        '*:document modes:_texniker_modes'
}

_texniker "$@"
