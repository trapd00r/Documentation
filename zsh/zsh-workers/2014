# Undefined functions.


# Defined functions.

chpwd () {
	settitle $(print -Pn "%n %m %~")
}
clean () {
	local ask=-i
	if [[ $# -eq 1 && $1 = -f* ]]
	then
		ask='-f' 
	fi
	setopt nullglob
	for f in core .*~ *~ \#*\#
	do
		[ -e $f ] && rm $ask $f
	done
	unsetopt nullglob
}
dvilw2 () {
	dvips -o\$lw2\ -R $*
}
dvilw3 () {
	dvips -o\$lw3\ -R $*
}
dvilw4 () {
	dvips -o\$lw4\ -R $*
}
dviqms () {
	dvips -o\$qms\ -R $*
}
emfaq () {
	waissearch -h bigbird.bu.edu -d faq -p 2210 $*
}
ipname () {
	nslookup -q=ptr $1
}
l. () {
	if [[ $# -eq 0 ]]
	then
		ls -aFd .*(N)
	else
		args= 
		for a
		do
			echo $a
			if [[ $a != -* ]]
			then
				a=$a/.*(N) 
			fi
			args="$args $a" 
		done
		ls -aFd $args
		args= 
	fi
}
mk () {
	touch make.log
	time make $* >& make.log
}
mkt () {
	mk $* &
	tail -f make.log
}
prlpr () {
	pr -l88 $* | textps -s9 -l88 | lpr
}
prlw4 () {
	pr -l88 $* | textps -s9 -l88 | lpr -Plw4
}
prpmc () {
	enscript -2rhG -fCourier8 $*
}
settitle () {
	print -n "\eP1.yConsole $3\e\\\\\eP3.yConsole $3\e\\\\"
}
unsh () {
	echo sed -n -e '/#! *\/bin.sh/,$p' $*
}
unshar () {
	sed -n -e '/#! *\/bin.sh/,$p' $* | /bin/sh
}
ztar () {
	zcat $2 | tar f$1 - $3*
}

